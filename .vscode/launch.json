{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch web app",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}/web",
			"preLaunchTask": "pub: build_runner serve"
		},
		{
			"name": "Serve pre-built app",
			"type": "dart",
			"request": "launch",
			"program": "bin/devtools.dart"
		}
	]
}
